<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Description</th>
            <th>Location</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: packages">
        <tr>
            <td data-bind="text: Id"></td>
            <td data-bind="text: Title"></td>
            <td data-bind="text: Description"></td>
            <td data-bind="text: Location"></td>
            <td><input type="button" class="btn btn-danger btn-xs" value=" [x] delete " data-bind="click: $parent.removePackage" /></td>
        </tr>
    </tbody>
</table>

<br />
<input type="button" class="btn btn-default" id="btnGetPackages" value="Refresh" data-bind="click: getPackages" />
